int main()
{
    int n;
    cin >> n;
int number=1;
    int i = 1;
    while (i <= n)
    {
        int space =i-1;
        while (space)
        {
            cout << " ";
            space--;
        }
        int j = 1;
        while (j <= n-i+1)
        {
            cout << number;
            j++;
        }
        cout << endl;
        i++;
        number++;
    }
}
