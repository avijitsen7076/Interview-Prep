#include <bits/stdc++.h>
int main()
{

    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin >> n;

        int i = 1;

        while (i <= n)
        {
            int number = i;
            int space = (n - i);
            while (space)
            {
                cout << " ";
                space--;
            }
            int j = 1;
            while (j <= i)
            {
                cout << number;
                number++;
                j++;
            }

            int start = i - 1;
            while (start)
            {
                cout << number - 2;
                number--;
                start--;
            }

            cout << endl;
            i++;
        }
    }
}
